{
  "name": "recranet/craft-recranet-console",
  "type": "craft-plugin",
  "require": {
    "php": ">=7.4",
    "craftcms/cms": ">=4.4.0"
  },
  "autoload": {
    "psr-4": {
      "recranet\\craftrecranetconsole\\": "src/"
    }
  },
  "extra": {
    "handle": "_craft-recranet-console",
    "name": "Recranet Console",
    "developer": "Recranet",
    "documentationUrl": ""
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}